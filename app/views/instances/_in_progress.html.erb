<%= render partial: "member_info", locals:{members: @members} %>

<div class="row mb-3">
  <div class="col-12">
    <h3>任務進度</h3>
  </div>
  <div class="col-12">
    <%= simple_form_for @instance, url:submit_instance_path(@instance), method: :post do |f| %>
      <%= f.input :answer %>
      <%= f.button :submit, '提交', class: "btn btn-primary" %>
    <%end%>
  </div>
</div>
